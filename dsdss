-- Utils random
math.randomseed(tick()*1e6%1e6)
local function randstr(n)
    local c = "QWERTYUIOPASDFGHJKLZXCVBNMabcdefghijklmnopqrstuvwxyz0123456789"
    local s = {}
    for i=1,n do
        local idx = math.random(1,#c)
        s[i] = string.sub(c, idx, idx)
    end
    return table.concat(s)
end

